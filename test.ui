<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Toplevel" id="TopLevel">
    <property name="height">200</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Frame" id="Main">
        <property name="height">200</property>
        <property name="width">500</property>
        <layout>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="ttk.Progressbar" id="MicroBar">
            <property name="length">400</property>
            <property name="mode">determinate</property>
            <property name="orient">horizontal</property>
            <property name="value">30</property>
            <layout>
              <property name="column">0</property>
              <property name="padx">25</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Progressbar" id="MacroBar">
            <property name="length">400</property>
            <property name="mode">determinate</property>
            <property name="orient">horizontal</property>
            <property name="value">30</property>
            <layout>
              <property name="column">0</property>
              <property name="ipady">10</property>
              <property name="padx">25</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="MicroLabel">
            <property name="text" translatable="yes">Uploading...</property>
            <layout>
              <property name="column">0</property>
              <property name="ipady">0</property>
              <property name="padx">25</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="MacroLabel">
            <property name="text" translatable="yes">Testing...</property>
            <layout>
              <property name="column">0</property>
              <property name="ipady">0</property>
              <property name="padx">25</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
